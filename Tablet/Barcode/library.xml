<?xml version="1.0" encoding="utf-8"?>
<library>
	<extension id="webworks.media.barcode">
		<entryClass>webworks.media.barcode.Barcode</entryClass>
	</extension>
	
	<platforms>
		<platform value="AIR">
			<target version="default" config="AIR_XHR" />
		</platform>
	</platforms>	
	
	<configurations>
		<configuration name="AIR_XHR" version="1.0" comment="For XHR architecture for Java implementation">			
			<src type="text/javascript" path="js/common" comment="Application JS interface" />
			<src type="text/actionscript" path="src/Air/Barcode/src" comment="ActionScript implementation" />
			<src type="text/actionscript" path="src/zxing/core/src" comment="ActionScript implementation" />
		</configuration>
	</configurations>	
	
	<features>
		<feature id="webworks.media.barcode" version="1.0.0" />
	</features>	
</library>	